{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome: localhost:3000",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Chrome: localhost:3000 (?test=1)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/?test=1",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Chrome: attach",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    }
  ]
}
